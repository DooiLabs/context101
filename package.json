{
  "name": "@context101/mcp",
  "version": "0.0.4",
  "description": "MCP server for Context101",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && chmod 755 dist/index.js",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "dev": "tsc -p tsconfig.build.json --watch",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DooiLabs/context101.git",
    "directory": "."
  },
  "license": "MIT",
  "type": "module",
  "bin": {
    "context101": "dist/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "fastmcp": "^3.25.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "typescript": "^5.8.2"
  }
}
